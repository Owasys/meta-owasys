+#!/bin/bash
+
+source /opt/crosstool/fsl-imx-xwayland/5.10-hardknott/environment-setup-cortexa53-crypto-poky-linux
+#export ARCH=arm64
+
+if [ -e './Install/' ]
+then
+   echo "Install directory does exist"
+else
+   mkdir -p ./Install/etc
+   mkdir -p ./Install/sbin
+fi
+
+make -j 11 clean
+make -j 11 distclean
+make -j 11 imx8mp_owa5x_defconfig
+make -j 11 
+
+cp ./spl/u-boot-spl.bin ./Install/u-boot-spl.bin
+cp ./u-boot-nodtb.bin ./Install/u-boot-nodtb.bin
+cp ./arch/arm/dts/imx8mp-owa5x.dtb ./Install/imx8mp-owa5x.dtb
+
+cp ./Install/u-boot-spl.bin ../imx-mkimage/iMX8M/u-boot-spl.bin
+cp ./Install/u-boot-nodtb.bin ../imx-mkimage/iMX8M/u-boot-nodtb.bin
+cp ./Install/imx8mp-owa5x.dtb ../imx-mkimage/iMX8M/imx8mp-owa5x.dtb
