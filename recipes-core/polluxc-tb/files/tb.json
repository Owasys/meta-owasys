{
   "name": "tb",
   "connection": {
      "protocol": "ssl",
      "host": "odm.owasys.com",
      "port": 8883,
      "trustfile": "/etc/ssl/certs/owasys_chain.pem",
      "keepalive": 2400,
      "retry": 3,
      "report_tlm": 10,
      "report_att": 2,
      "tcp_keepalive": 10,
      "time_off": 60,
      "time_on": 30
   },
   "auth": {
      "client-id": "",
      "token": "123456",
      "certfile": "/device/odm/client.pem",
      "secret": "/device/odm/secret"
   },
   "attributes": {
      "clientKeys": "",
      "sharedKeys": "fw_title,fw_version,fw_size,fw_checksum,fw_checksum_algorithm,sw_title,sw_version,sw_size,sw_checksum,sw_checksum_algorithm"
   },
   "comms": {
      "owadPid": "/var/run/tb_commands.pid"
   },
   "objects": [
      "",
      ""
   ],
   "redis": {
      "q-in": "tb-in",
      "q-in-cli": "tb-in-cli",
      "q-out": "tb-out",
      "rpc-in": "tb-rpc-in",
      "rpc-out": "tb-rpc-out",
      "telemetry": "tb-telemetry",
      "msg-len": 5,
      "send_email_notification": false
   },
   "bootstrap": {
      "protocol": "ssl",
      "host": "dm.owasys.com",
      "port": 8884,
      "trustfile": "/etc/ssl/certs/owasys_chain.pem",
      "keepalive": 300,
      "version": 1
   }
}